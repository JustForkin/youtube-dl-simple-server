{
  "name": "youtube-dl-simple-server browser plugin",
  "description": "Adds a button to run youtube-dl-simple-server",
  "version": "0.2",
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Try to download this video",
      "default_icon": "ybp_16x16.png"
  },
  "manifest_version": 2
}
